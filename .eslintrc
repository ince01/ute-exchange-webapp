{
  "extends": ["airbnb", "react-app", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/jsx-props-no-spreading": 0,
    "react/no-array-index-key": 0,
    "comma-dangle": 0,
    "semi": 0,
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": ["state"]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0
  }
}
